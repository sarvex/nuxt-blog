    "@adonisjs/cors": "1.0.7",
    "@adonisjs/fold": "8.2.0",
    "@adonisjs/framework": "5.0.13",
    "@adonisjs/ignitor": "2.0.8",
    "@adonisjs/lucid": "18.4.2",
    "@adonisjs/session": "6.4.0",
    "@adonisjs/shield": "7.1.1",
    "vuetify": "3.4.4",
    "vuetify-loader": "1.9.2",
