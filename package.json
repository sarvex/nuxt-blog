    "@adonisjs/fold": "10.1.3",
    "@adonisjs/framework": "5.0.13",
    "@adonisjs/ignitor": "2.0.8",
    "@adonisjs/lucid": "21.6.0",
    "@adonisjs/session": "7.5.1",
    "@adonisjs/shield": "8.1.2",
    "vuetify": "3.7.15",
