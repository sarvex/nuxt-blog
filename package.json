    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "prettier": "3.1.0",
    "stylus": "0.62.0",
    "stylus-loader": "7.1.3",
    "@vue/test-utils": "2.4.3",
